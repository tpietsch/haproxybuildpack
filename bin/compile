#!/usr/bin/env bash∆í
echo "-----&gt; haproxy-1.5.18"
# change to the the BUILD_DIR ($1)
cd $1
curl -O http://www.haproxy.org/download/1.5/src/haproxy-1.5.18.tar.gz
tar -zxvf haproxy-1.5.18.tar.gz
rm haproxy-1.5.18.tar.gz
touch haproxy.pid
cd haproxy-1.5.18
make TARGET=linux

